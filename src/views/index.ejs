<h1 class="mt-5 mb-4">Project</h1>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Author</th>
            <th scope="col">Date</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        <% projects.forEach(project=> { %>
            <tr>
                <th scope="row">
                    <%= project.id %>
                </th>
                <td>
                    <%= project.name %>
                </td>
                <td>
                    <%= project.desc %>
                </td>
                <td>
                    <%= project.author %>
                </td>
                <td>
                    <%= project.date %>
                </td>
                <td>
                    <a href="/update-project/<%= project.id %>" class="btn btn-warning">Update</a>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>
</div>