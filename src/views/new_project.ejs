
<div id ="project_box">
  <h2 id="form_heading">Project Information</h2>
  <form action="/" method="post" id="project_form">
    <label for="name">Name of the project:</label><br>
    <input type="text" id="project_name" name="name" required><br>
    <label for="desc">Description:</label><br>
    <textarea  id="project_desc" name="desc" rows="3" required></textarea> 
    <label for="author">Author:</label>
    <input type="text" id="author" name="author" required><br>
    <label for="Date">Date:</label> 
    <input type="date"id="date" name="date" required>
     
     <div id="box">
          <button type="submit" class="form_button" id="close"><a href="#">Close</a> </button>
          <button type="submit" class="form_button" id="create">Create</button>
     </div>
  </form>
</div>

